{
    "name": "xmexcel-model",
    "version": "0.7.0",
    "description": "excel state",
    "author": "ximing",
    "main": "./dist/index.js",
    "scripts": {
        "i": "SASS_BINARY_SITE=https://npm.taobao.org/mirrors/node-sass/ npm install --registry=https://registry.npm.taobao.org",
        "dev": "./node_modules/.bin/webpack-dev-server ",
        "prepublish": "npm run build",
        "builddemo": "webpack --config webpack.build.demo.config.js",
        "buildlib": "webpack --config webpack.build.lib.config.js",
        "builddll": "webpack --config webpack.dll.config.js",
        "build": "rimraf dist && mkdir dist && rsync -va -progress  ./src/* ./dist && babel --presets=es2015,stage-0,react  --plugins=transform-decorators-legacy src --out-dir dist",
        "test-watch": "export NODE_ENV=test && ava --watch",
        "test": "export NODE_ENV=test && nyc ava"
    },
    "keywords": [],
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/ximing/dxexcel-state"
    },
    "devDependencies": {
        "autoprefixer": "^7.2.5",
        "ava": "^0.25.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.1",
        "babel-loader": "7.1.2",
        "babel-plugin-import": "^1.6.3",
        "babel-plugin-syntax-async-functions": "^6.13.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-export-extensions": "^6.22.0",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "cross-env": "^5.1.3",
        "css-loader": "^0.28.9",
        "env-cmd": "^7.0.0",
        "eslint": "^4.16.0",
        "eslint-plugin-react": "^7.6.0",
        "file-loader": "^1.1.6",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "html-webpack-plugin": "^2.30.1",
        "node-sass": "^4.7.2",
        "nyc": "^11.4.1",
        "postcss": "^6.0.16",
        "postcss-clearfix": "^2.0.1",
        "postcss-loader": "^2.0.10",
        "postcss-position": "^1.0.0",
        "postcss-size": "^2.0.0",
        "rabjs": "^3.6.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-hot-loader": "^3.1.3",
        "regenerator-runtime": "^0.11.1",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.20.1",
        "url-loader": "^0.6.2",
        "webpack": "^3.10.0",
        "webpack-dev-server": "^2.11.1"
    },
    "files": [
        "packages",
        "src",
        "dist",
        "example",
        "README.md"
    ],
    "engines": {
        "node": ">= 5.0.0",
        "npm": ">= 3.0.0"
    },
    "ava": {
        "files": [
            "test/*.js",
            "test/change/*.js"
        ],
        "source": [
            "src/**/*.{js,jsx}",
            "test/*.js",
            "!dist/**/*",
            "!node_modules/**/*"
        ],
        "match": [
            "*",
            "!foo"
        ],
        "concurrency": 5,
        "failFast": true,
        "failWithoutAssertions": false,
        "tap": true,
        "powerAssert": false,
        "require": [
            "babel-register"
        ],
        "babel": "inherit"
    },
    "dependencies": {
        "lodash": "^4.17.5",
        "shortid": "^2.2.8"
    }
}
